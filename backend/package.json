{
  "name": "destination-kolkata-backend",
  "version": "1.0.0",
  "description": "Backend API server for Destination Kolkata",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "ingest-data": "tsx scripts/ingest-data.ts",
    "load-data": "tsx scripts/load-data.ts",
    "data-manager": "tsx scripts/data-manager.ts",
    "data-manager:ingest": "tsx scripts/data-manager.ts ingest-and-load",
    "data-manager:load": "tsx scripts/data-manager.ts load-existing",
    "data-manager:fresh": "tsx scripts/data-manager.ts fresh-ingest",
    "seed-db": "tsx scripts/seed-database.ts",
    "seed-admin": "tsx scripts/seed-admin-user.ts",
    "verify-admin": "node scripts/verify-admin.js",
    "generate-admin": "node scripts/generate-admin-info.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "api",
    "backend",
    "express",
    "typescript",
    "mongodb",
    "kolkata",
    "tourism"
  ],
  "author": "Pranab Paul",
  "license": "MIT"
}
